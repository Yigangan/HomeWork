<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tjut.mapper.FundMapper">

    <resultMap id="fundMap" type="com.tjut.pojo.Fund">
        <id column="fund_code" property="fundCode"/>
        <result column="end_date" property="endDate"/>
        <result column="unit_net_val" property="unitNetVal"/>
    </resultMap>
    <select id="getRecentFundList" resultMap="fundMap">
        SELECT a.fund_code,a.end_date,a.unit_net_val
        FROM tb_fund AS a,(
            SELECT fund_code,MAX(end_date) AS end_date
            FROM tb_fund
            GROUP BY fund_code
        ) AS b
        WHERE a.fund_code = b.fund_code AND a.end_date = b.end_date;
    </select>
</mapper>